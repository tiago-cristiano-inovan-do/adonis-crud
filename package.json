{
  "name": "@inovan.do/adonis-crud",
  "version": "0.0.12",
  "description": "A crud abstraction for AdonisJs",
  "main": "build/providers/AdonisCrudProvider.js",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "clean": "del-cli build",
    "copyfiles": "copyfiles \"templates/**/*.*\" \"instructions.md\" build",
    "compile": "npm run lint && npm run clean && tsc --sourceMap --preserveWatchOutput && npm run copyfiles",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np --message=\"chore(release): %s\"",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json {owner}/{skeleton}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inovando/adonis-crud.git"
  },
  "homepage": "https://github.com/inovando/adonis-crud#readme",
  "keywords": [
    "adonisjs",
    "adonis"
  ],
  "author": "Tiago Cristiano Alves",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inovando/adonis-crud/issues"
  },
  "devDependencies": {
    "@adonisjs/core": "^5.8.4",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.12",
    "@adonisjs/sink": "^5.4.0",
    "@types/node": "^18.0.3",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "github-label-sync": "^2.2.0",
    "husky": "^8.0.1",
    "inquirer-file-tree-selection-prompt": "^2.0.5",
    "mrm": "^4.0.0",
    "np": "^7.6.2",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.8.4",
    "@adonisjs/lucid": "^18.1.1",
    "plop": "^3.1.1"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [],
    "minNodeVersion": "16.13.1",
    "probotApps": []
  },
  "types": "build/adonis-typings/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-debugger": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "publishConfig": {
    "access": "public"
  },
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "instructionsMd": "./build/instructions.md",
    "providers": [
      "@inovan.do/adonis-crud",
      "./providers/AuthRepositoryProvider",
      "./providers/UserRepositoryProvider"
    ],
    "types": "@inovan.do/adonis-crud",
    "templates": {
      "basePath": "./build/templates",
      "contracts": [
        {
          "src": "contracts/context.txt",
          "dest": "contracts"
        }
      ]
    }
  },
  "dependencies": {
    "@adonisjs/lucid": "^18.1.1",
    "adonis-bumblebee-ts": "^1.0.0",
    "fs-extra": "^10.1.0",
    "plop": "^3.1.1"
  },
  "directories": {
    "doc": "docs"
  }
}
